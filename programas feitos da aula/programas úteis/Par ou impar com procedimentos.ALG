Algoritmo "semnome"

Var

 N: Inteiro

Procedimento ParOuImpar(V: Inteiro)
inicio
      Se (V%2 = 0) entao
         Escreval("O numero ", V," é PAR")
      senao
         Escreval("O numero ", V," é IMPAR")
      fimse
FimProcedimento


Inicio
    escreval("Digite um número: ")
    leia(N)
    ParOuImpar(N)
    

Fimalgoritmo