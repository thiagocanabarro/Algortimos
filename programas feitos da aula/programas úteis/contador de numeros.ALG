Algoritmo "semnome"

Var
   valor,contador: inteiro
Inicio
     contador <- 1

     Escreval("Até que número deseja contar ?")
     leia(valor)
     
     Enquanto (contador <= valor) faca
     Escreval(contador)
     contador <- contador +1

     fimenquanto
     
     Escreval("Fim da contagem")

Fimalgoritmo